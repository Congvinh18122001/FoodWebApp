@model IEnumerable<Food.lib.Objects.Product>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="main">
    <h3><a href="#">&nbsp;<span>Suppliers </span></a></h3>
    <div class="section group">
        @foreach (var item in Model)
        {
            <div class="col images_1_of_3">
                <img src="~/Content/User/images/@item.Image" alt="@item.ProductName" />
                <h2>@item.ProductName -- @item.Supplier.SupplierName</h2>
                <p>Address : @item.Supplier.Address</p>
                <a href="@Url.Action("Detail","Service",new { id = item.Id })" > <div class="btn2"></div></a>
            </div>
        }

    </div>
</div>